<%= form_for @company do |f| %>

<%= f.label :name %>
<%= f.text_field :name %>

<%= f.label :suffix %>
<%= f.text_field :suffix %>

<%= f.label :catch_phrase %>
<%= f.text_field :catch_phrase %>

<%= f.label :email %>
<%= f.text_field :email %>

<%= f.label :start_date %>
<%= f.text_field :start_date %>

<%= f.label :industry %>
<%= f.text_field :industry %>

<%= f.submit %>

<% end %>
