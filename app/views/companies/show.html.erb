  <thead>
    <th>Department</th>
    <th>Name</th>
    <th>Price</th>
    <th>Company Id</th>
  </thead>
  <tbody>
    <% @company.products.each do |p| %>
    <tr>
      <td><%= p.department %></td>
      <td><%= p.name %></td>
      <td><%= p.price %></td>
    </tr>
    <% end %>
  </tbody>
</table>
