<table>
  <thead>
    <th>Name</th>
    <th>Suffix</th>
    <th>Catch Phrase</th>
    <th>email</th>
    <th>Start Date</th>
    <th>Industry</th>
  </thead>
  <tbody>
      <tr>
        <td><%= @company.name %></td>
        <td><%= @company.suffix %></td>
        <td><%= @company.catch_phrase %></td>
        <td><%= @company.email %></td>
        <td><%= @company.start_date %></td>
        <td><%= @company.industry %></td>
      </tr>
  </tbody>
</table>
